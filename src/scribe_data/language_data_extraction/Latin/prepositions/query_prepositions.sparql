# tool: scribe-data
# All Latin (Q397) prepositions and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  ?lexeme  # unique ID for the data entry
  ?preposition  # lemma or label of the ID

WHERE {
  ?lexeme dct:language wd:Q397 ;  # Latin language
    wikibase:lexicalCategory wd:Q4833830 ;  # prepositions
    wikibase:lemma ?preposition .
}